			<!-- Box分解(ピック)出庫実績 --><PickedShipActual>0</PickedShipActual>
			<!-- Box在庫 --><BoxInTransit>0</BoxInTransit>
			<!-- Box輸送中在庫 --><BoxInTransitDetails>0</BoxInTransitDetails>
		</Event>
		<Log>
			<!-- 企業品目 --><!-- CompanyItem>0</CompanyItem -->
			<!-- 入庫予定 --><ArrivalSchedule>0</ArrivalSchedule>	
			<!-- 注文 --><PurchaseOrder>0</PurchaseOrder>	
			<!-- 販売計画 --><SalesPlan>0</SalesPlan>	
			<!-- 供給計画 --><SupplyPlan>0</SupplyPlan>	
			<!-- 調達計画 --><ProcPlan>0</ProcPlan>	
			<!-- 注残 --><Backlog>0</Backlog>	
			<!-- 出庫指示 --><ShipInstruction>0</ShipInstruction>	
			<!-- 実需 --><ActualDemand>0</ActualDemand>	
			<!-- 需要予測 --><DemandForecast>0</DemandForecast>	
			<!-- 生産計画 --><ProdPlan>0</ProdPlan>	
			<!-- 検査計画 --><InspectionPlan>0</InspectionPlan>
			<!-- 生産指示 --><ProdInstruction>0</ProdInstruction>	
			<!-- 検査指示 --><InspectionInstruction>0</InspectionInstruction>	
			<!-- 支払予定 --><PaymentProgram>0</PaymentProgram>	
			<!-- 安全在庫 --><SafetyStock>0</SafetyStock>	
			<!-- AgentJobLog --><AgentJobLog>0</AgentJobLog>	
			<!-- ResourceMaster --><ResourceMaster>0</ResourceMaster>	
			<!-- ToolMaster --><ToolMaster>0</ToolMaster>	
			<!-- ResourceLog --><!-- ResourceLog>0</ResourceLog -->	
			<!-- ResourceStatus --><ResourceStatus>0</ResourceStatus>	
			<!-- ResourceSharingResults --><ResourceSharingResults>0</ResourceSharingResults>	
			<!-- ToolLog --><!-- ToolLog>0</ToolLog -->	
		</Log>
	</LogOutputControl>

	<LogSaveControl>
		<Daily>
			<!-- 口座残高 --><AccountBalance>1</AccountBalance>
			<!-- 需要実績 --><ActualDemand>1</ActualDemand>
			<!-- 入庫実績 --><ArrivalActual>1</ArrivalActual>
			<!-- 出庫実績 --><ShipActual>1</ShipActual>
			<!-- 入金実績 --><ReceivingActual>1</ReceivingActual>
			<!-- 出金実績 --><PaymentActual>1</PaymentActual>
			<!-- 売掛残高 --><SalesBalance>1</SalesBalance>
			<!-- 買掛残高 --><PaymentBalance>1</PaymentBalance>
			<!-- 生産実績 --><ProdActual>1</ProdActual>
			<!-- 検査実績 --><InspectionActual>1</InspectionActual>
			<!-- 仕掛量 --><WIP>1</WIP>
			<!-- 在庫 --><InTransit>1</InTransit>
			<!-- 注残 --><Backlog>1</Backlog>
			<!-- 保管量 --><InventoryActual>1</InventoryActual>
			<!-- 輸送中在庫 --><InTransitDetails>1</InTransitDetails>
