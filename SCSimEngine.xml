			<!-- 保管量 --><InventoryActual>1</InventoryActual>
			<!-- 輸送中在庫 --><InTransitDetails>1</InTransitDetails>
			<!-- 在庫不足余裕度 --><StockShortageMargin>1</StockShortageMargin>
			<!-- 消失履歴 --><LostHistory>1</LostHistory>
			<!-- 標準原価 --><CostAccounting>1</CostAccounting>
			<!-- コンテナコスト --><ContainerCost>1</ContainerCost>
			<!-- コンテナ積載詳細 --><ContainerLoadingDetails>1</ContainerLoadingDetails>
			<!-- Box品入庫実績 --><BoxArrivalActual>1</BoxArrivalActual>
			<!-- Box品出庫実績 --><BoxShipActual>1</BoxShipActual>
