		<Event>
			<!-- 口座残高 --><AccountBalance>0</AccountBalance>
			<!-- 需要実績 --><ActualDemand>0</ActualDemand>
			<!-- 入庫実績 --><ArrivalActual>0</ArrivalActual>
			<!-- 出庫実績 --><ShipActual>0</ShipActual>
			<!-- 入金実績 --><ReceivingActual>0</ReceivingActual>
			<!-- 出金実績 --><PaymentActual>0</PaymentActual>
			<!-- 売掛残高 --><SalesBalance>0</SalesBalance>
			<!-- 買掛残高 --><PaymentBalance>0</PaymentBalance>
			<!-- 生産実績 --><ProdActual>0</ProdActual>
			<!-- 検査実績 --><InspectionActual>0</InspectionActual>
			<!-- 仕掛量 --><WIP>0</WIP>
			<!-- 在庫 --><InTransit>0</InTransit>
			<!-- 注残 --><Backlog>0</Backlog>
			<!-- 保管量 --><InventoryActual>0</InventoryActual>
			<!-- 輸送中在庫 --><InTransitDetails>0</InTransitDetails>
			<!-- 在庫不足余裕度 --><StockShortageMargin>0</StockShortageMargin>
