			<!-- 支払予定 --><PaymentProgram>1</PaymentProgram>	
			<!-- AgentJobLog --><AgentJobLog>1</AgentJobLog>	
			<!-- 安全在庫 --><SafetyStock>1</SafetyStock>	
		</Log>
	</LogSaveControl >

	<Dir>
		<InputDir> ./data </InputDir>
		<OutputDir> ./out </OutputDir>
		<LogDir> ./log </LogDir>
	</Dir>
	<Agent>
		<!-- TriggerIntdays> 0 </TriggerIntdays -->
		<TriggerIntdays> 1 </TriggerIntdays>
		<!-- TriggerPauseMinute> 1 </TriggerPauseMinute -->

	</Agent>
	<Log>
		<!-- 個体管理ログ出力 未出力:=0/省略、通常:=1、ブロック型:=2 -->
		<DispTraceManage> 1 </DispTraceManage>

		<!--デバッグ用出力  -->
		<!--出庫指示A開始/終了時刻出力 未出力:=0/省略、出力:=1 -->
		<DebugShipInstAgent> 0 </DebugShipInstAgent>
		<!--出庫イベント開始/終了時刻出力 未出力:=0/省略、出力:=1 -->
		<DebugShipEvent> 0 </DebugShipEvent>
		<!--入庫イベント開始/終了時刻出力 未出力:=0/省略、出力:=1 -->
		<DebugArrivalEvent> 0 </DebugArrivalEvent>
		<!--エージェント開始/終了時刻出力 未出力:=0/省略、出力:=1 -->
		<DebugAllAgent> 0 </DebugAllAgent>

	</Log>
	<Container>
		<!-- 最小有効量の算出用除数桁数(省略時…2) -->
		<!-- 例…2→1/100 (10^-2) -->
		<MinEffectNumDigit> 3 </MinEffectNumDigit>
		<!-- 全コンテナ容量節約モード
			(省略時…0(通常モード) 
			 		1(全コンテナ廃棄モード)  -->
		<SaveBufferMode>0</SaveBufferMode>
	</Container>
	<Box>
		<!-- BoxOFR下限省略時値(省略時…0.0) -->
		<!-- 値範囲:0.0～1.0 -->
		<!-- BoxOFR>0.3</BoxOFR -->
		<BoxOFR>0.0</BoxOFR>
	</Box>
	<Loader>
		<!-- 分割入力ファイル確認モード
			(省略時…0(分割ファイルを確認しない…通常モード) 
			 		1(確認する)  -->
		<SplitLoadFileCheck>0</SplitLoadFileCheck>
	</Loader>
	<!-- 省略時為替レート指定
			(CompanyIS.csv中CurrencyID省略(空欄)時使用される為替レート)
			省略時…1.0  -->
	<DefaultExchangeRate>
		1.0
	</DefaultExchangeRate>

	<!-- SimulationLog出力制御 -->
	<!-- 標準出力のみ出力:=0/省略、ファイルのみ出力:=1 標準出力/ファイル双方出力:=2-->
	<SimulationLogOutput>1</SimulationLogOutput>

	<LogInputControl>
		<!-- デリミタ指定 カンマ:=0/省略、タブ:=1 -->
		<Delimiter>0</Delimiter>
	</LogInputControl>

</SCMEngineSettings>
