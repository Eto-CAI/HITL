			<!-- Box分解(ピック)出庫実績 --><PickedShipActual>1</PickedShipActual>
			<!-- Box在庫 --><BoxInTransit>1</BoxInTransit>
			<!-- Box輸送中在庫 --><BoxInTransitDetails>1</BoxInTransitDetails>
		</Event>
		<Log>
			<!-- 入庫予定 --><ArrivalSchedule>1</ArrivalSchedule>	
			<!-- 注文 --><PurchaseOrder>1</PurchaseOrder>	
			<!-- 販売計画 --><SalesPlan>1</SalesPlan>	
