			<!-- 個体管理 --><IDLog>1</IDLog>
			<!-- コンテナコスト --><ContainerCost>1</ContainerCost>
			<!-- コンテナ積載詳細 --><ContainerLoadingDetails>1</ContainerLoadingDetails>
			<!-- Box品入庫実績 --><BoxArrivalActual>1</BoxArrivalActual>
			<!-- Box品出庫実績 --><BoxShipActual>1</BoxShipActual>
			<!-- Box品保管量 --><BoxInventoryActual>1</BoxInventoryActual>
			<!-- 注文取消し履歴 --><OrderCancelHistory>1</OrderCancelHistory>
			<!-- Box分解(ピック)出庫実績 --><PickedShipActual>1</PickedShipActual>
			<!-- Box在庫 --><BoxInTransit>1</BoxInTransit>
			<!-- Box輸送中在庫 --><BoxInTransitDetails>1</BoxInTransitDetails>
			<!-- 炭素排出量 --><CO2Emission>1</CO2Emission>
		</Daily>
		<Event>
			<!-- 口座残高 --><AccountBalance>1</AccountBalance>
			<!-- 需要実績 --><ActualDemand>1</ActualDemand>
			<!-- 入庫実績 --><ArrivalActual>1</ArrivalActual>
			<!-- 出庫実績 --><ShipActual>1</ShipActual>
			<!-- 入金実績 --><ReceivingActual>1</ReceivingActual>
			<!-- 出金実績 --><PaymentActual>1</PaymentActual>
			<!-- 売掛残高 --><SalesBalance>1</SalesBalance>
			<!-- 買掛残高 --><PaymentBalance>1</PaymentBalance>
			<!-- 生産実績 --><ProdActual>1</ProdActual>
			<!-- 検査実績 --><InspectionActual>1</InspectionActual>
			<!-- 仕掛量 --><WIP>1</WIP>
			<!-- 在庫 --><InTransit>1</InTransit>
